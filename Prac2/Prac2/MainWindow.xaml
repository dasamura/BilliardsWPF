        xmlns:helix="clr-namespace:HelixToolkit.Wpf;assembly=HelixToolkit.Wpf"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <helix:HelixViewport3D Name="helixViewport">
     
            
            <ModelVisual3D Content="{Binding YourModel3D1}" />
